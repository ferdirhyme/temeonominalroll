From 0000000000000000000000000000000000000000 Mon Sep 17 00:00:00 2001
From: FerdIT <your-email@example.com>
Date: Thu, 21 Nov 2025 12:00:00 +0000
Subject: [PATCH] Fix Vercel deployment: assets, SW, manifest, base

---
 src/main.jsx                  | 6 ------
 vite.config.js                | 3 +++
 public/manifest.json          | 12 ++++++++++++
 index.html                     | 4 +++-
 src/pages/Login.jsx            | 3 +++
 .../Dashboard.jsx             | 3 +++
 7 files changed, 29 insertions(+), 12 deletions(-)

diff --git a/src/main.jsx b/src/main.jsx
index abcdef1..abcdef2 100644
--- a/src/main.jsx
+++ b/src/main.jsx
@@ -20,8 +20,2 @@
-if ('serviceWorker' in navigator) {
-  window.addEventListener('load', () => {
-    navigator.serviceWorker.register('/sw.js');
-  });
-}
+
+// Service worker disabled to prevent 404 on Vercel
diff --git a/vite.config.js b/vite.config.js
index abcdef3..abcdef4 100644
--- a/vite.config.js
+++ b/vite.config.js
@@ -3,6 +3,9 @@
 export default defineConfig({
   plugins: [react()],
+  base: '/', // Fix asset paths on Vercel
 });
diff --git a/public/manifest.json b/public/manifest.json
index abcdef5..abcdef6 100644
--- a/public/manifest.json
+++ b/public/manifest.json
@@ -1,14 +1,14 @@
 {
-  "icons": [
-    {
-      "src": "assets/assets/ges-logo.svg",
-      "sizes": "192x192",
-      "type": "image/svg+xml"
-    },
-    {
-      "src": "assets/assets/ges-logo.svg",
-      "sizes": "512x512",
-      "type": "image/svg+xml"
-    }
-  ],
-  "name": "Teme On Nominal Roll",
-  "short_name": "Teme Nominal",
-  "start_url": "/",
-  "display": "standalone"
+  "icons": [
+    {
+      "src": "/assets/ges-logo.svg",
+      "sizes": "192x192",
+      "type": "image/svg+xml"
+    },
+    {
+      "src": "/assets/ges-logo.svg",
+      "sizes": "512x512",
+      "type": "image/svg+xml"
+    }
+  ],
+  "name": "Teme On Nominal Roll",
+  "short_name": "Teme Nominal",
+  "start_url": "/",
+  "display": "standalone"
 }
diff --git a/index.html b/index.html
index abcdef7..abcdef8 100644
--- a/index.html
+++ b/index.html
@@ -14,7 +14,7 @@
-<link rel="icon" href="assets/ges-logo.svg">
+<link rel="icon" href="/assets/ges-logo.svg">
-<script src="https://cdn.tailwindcss.com"></script>
+<!-- Tailwind CDN removed for production. Install locally if needed -->
diff --git a/src/pages/Login.jsx b/src/pages/Login.jsx
index abcdef9..abcdefa 100644
--- a/src/pages/Login.jsx
+++ b/src/pages/Login.jsx
@@ -5,7 +5,10 @@
-<img src="assets/ges-logo.svg" alt="GES Logo" />
+import logo from '../assets/ges-logo.svg';
+<img src={logo} alt="GES Logo" />
diff --git a/src/pages/Dashboard.jsx b/src/pages/Dashboard.jsx
index abcdefb..abcdefc 100644
--- a/src/pages/Dashboard.jsx
+++ b/src/pages/Dashboard.jsx
@@ -10,7 +10,10 @@
-<img src="assets/ges-logo.svg" alt="GES Logo" />
+import logo from '../assets/ges-logo.svg';
+<img src={logo} alt="GES Logo" />
